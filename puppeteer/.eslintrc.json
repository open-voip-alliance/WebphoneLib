{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parser": "espree",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "rules": {
    "curly": ["error", "all"],
    "brace-style": ["error", "1tbs"],
    "indent": ["warn", 2],
    "no-console": "off",
    "prettier/prettier": "error"
  }
}
