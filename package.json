{
  "name": "webphone-lib",
  "version": "0.2.21",
  "description": "Webphone Lib",
  "author": "Open VoIP Alliance",
  "license": "MIT",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -fR dist/* build/*",
    "build": "npm run clean && rollup -c",
    "build-watch": "npm run clean && rollup -cw",
    "serve": "serve -l 1235",
    "demo": "run-p build-watch serve",
    "typecheck": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "prepare": "npm run lint && npm run build",
    "test": "ava test/*",
    "test-coverage": "nyc ava",
    "test-watch": "ava --watch",
    "docs": "typedoc --out docs src",
    "run-e2e-tests-in-docker": "docker-compose up --exit-code-from",
    "test-e2e-debug": "npx playwright test --project=chromium --debug"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "ava": {
    "require": [
      "esm",
      "ts-node/register",
      "./test/_setup-browser-env.js"
    ],
    "extensions": [
      "ts"
    ],
    "babel": false,
    "compileEnhancements": false
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-voip-alliance/WebphoneLib.git"
  },
  "files": [
    "src",
    "dist",
    "test",
    "README.md"
  ],
  "dependencies": {
    "p-retry": "^4.1.0",
    "p-timeout": "^3.2.0",
    "sip.js": "0.15.6"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@playwright/test": "^1.27.1",
    "@types/events": "^3.0.0",
    "@types/sinon": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "ava": "^2.4.0",
    "ava-fast-check": "^1.1.2",
    "base": "^0.13.2",
    "browser-env": "^3.2.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fast-check": "^1.17.0",
    "husky": "^3.0.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^1.11.1",
    "rollup": "^1.23.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "serve": "^11.3.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  }
}
